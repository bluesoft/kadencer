<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<p>
  <b>Has iterations:</b>
  <%= @project.has_iterations %>
</p>

<p id="organization">
  <b>Organization:</b>
  <%= @project.organization.name %>
</p>

<div id="users">
	<p>
		<b>Users:</b>
		<ul>
			<% @project.organization.users.each { |user| %>
				<li><%= user.fullName %></li>
			<% } %>
		</ul>
	</p>
</div>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
